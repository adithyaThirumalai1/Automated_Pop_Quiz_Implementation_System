<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teacherfinal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ListStudentActivity"></activity>
        <activity android:name=".BarChartActivity" />
        <activity android:name=".ListQuizActivity" />
        <activity android:name=".PieChartActivity" />
        <activity android:name=".DeployQuizActivity" />
        <activity android:name=".TrueFalseQuizActivity" />
        <activity android:name=".QuizTitleActivity" />
        <activity android:name=".McqQuizActivity" />
        <activity android:name=".SelectTypeActivity" />
        <activity android:name=".LoggedInActivity" />
        <activity android:name=".TeacherRegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>